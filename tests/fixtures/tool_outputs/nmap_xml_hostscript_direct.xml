<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE nmaprun>
<nmaprun scanner="nmap" args="nmap -sC -oX - 192.168.1.50">
  <host>
    <status state="up"/>
    <address addr="192.168.1.50" addrtype="ipv4"/>
    <script id="broadcast-ping" output="Host 192.168.1.1 is up."/>
    <ports></ports>
  </host>
</nmaprun>
